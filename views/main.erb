<html>
<head>
<style type="text/css">

html {
	background-color: #000;
}

#myTypingText {
	background-color:#000;
	width:100%;
	height:100%;
	margin: 70px auto;
	padding: 10px;
	color: #00FF00;
	font-family: monospace, sans-serif;
  font-size:26px;
	line-height:1.5em;
}
</style>
</head>
<body>
<div id="myTypingText"></div>
<script type="text/javascript" language="javascript">
var myString = "Hi, Matt Johnson. What are you doing talking to Jen Z. for that position? You're making the interwebs angry. You don't want to know what happens when the interwebs get angry...";
var myArray = myString.split("");
var loopTimer;
function frameLooper() {
	if(myArray.length > 0) {
		document.getElementById("myTypingText").innerHTML += myArray.shift();
	} else {
		clearTimeout(loopTimer); 
	}
	loopTimer = setTimeout('frameLooper()',80);
}
frameLooper();
</script>

<img src="/img/matrixman.gif"


</body>
</html>